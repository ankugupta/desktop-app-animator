{
  "appId": "com.prozo.desktop.book.animator",
  "productName": "Prozo",
  "directories": {
    "output": "release/"
  },
  "files": [
    "**/*",
    "!**/*.ts",
    "!**/*.js.map",
    "!*.code-workspace",
    "!LICENSE.md",
    "!README.md",
    "!package.json",
    "!package-lock.json",
    "!src/",
    "!e2e/",
    "!hooks/",
    "!angular.json",
    "!_config.yml",
    "!karma.conf.js",
    "!dev-notes/",
    "!browserslist",
    "!**/{angular.json, angular.webpack.js, tsconfig.spec.json, tslint.json, tsconfig-serve.json, tsconfig.app.json, tsconfig.json}"
  ],
  "win": {
    "icon": "dist/DigiBook",
    "target": [
      "nsis"
    ]
  },
  "mac": {
    "icon": "dist/DigiBook",
    "target": [
      "dmg"
    ]
  },
  "linux": {
    "icon": "dist/DigiBook",
    "target": [
      "AppImage"
    ]
  }
}